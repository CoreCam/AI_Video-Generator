# Environment variables template
# Copy this file to .env and fill in your actual values

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-flash

# Google Cloud/Veo Configuration  
GOOGLE_PROJECT_ID=your_google_project_id
VEO_MODEL=veo-3.1-generate-preview
VEO_USE_REFERENCE_IMAGES=true
GOOGLE_ACCESS_TOKEN=your_google_access_token_here

# Optional: AWS S3 Configuration (if using S3 storage)
# S3_BUCKET_NAME=your_bucket_name
# S3_ACCESS_KEY=your_s3_access_key
# S3_SECRET_KEY=your_s3_secret_key
# S3_REGION=us-east-1

# Optional: Application Security
# SECRET_KEY=your_secret_key_for_production